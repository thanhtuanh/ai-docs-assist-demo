{
  "name": "ai-doc-assist-frontend",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve --proxy-config proxy.conf.json",
    "start:prod": "ng serve --configuration production --proxy-config proxy.conf.json",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "test": "ng test",
    "test:backend": "npm run test:api && npm run test:integration",
    "test:api": "curl -f http://localhost:8080/api/ai/health || echo 'Backend not running'",
    "test:integration": "curl -f http://localhost:4200/api/ai/health || echo 'Proxy not working'",
    "dev:full": "concurrently \"npm run start\" \"echo 'Frontend started on http://localhost:4200'\"",
    "health:check": "curl -f http://localhost:8080/api/test/health && echo 'Backend healthy'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/router": "^18.2.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.3",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.0",
    "@angular/cli": "^18.2.0",
    "@angular/compiler-cli": "^18.2.0",
    "@types/node": "^20.14.15",
    "@types/jasmine": "~5.1.4",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.5.4"
  }
}